<?xml version="1.0" encoding="UTF-8"?>
<ContentView
xmlns="http://xamarin.com/schemas/2014/forms"
xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
x:Class="Branderra.StarOnlyLabeView">
	<ContentView.Content>
		<StackLayout HorizontalOptions="EndAndExpand" Orientation="Horizontal" HeightRequest="30">
	      <Grid>
	        <Image x:Name="starBlankOne" Source="star_outline.png" />
	        <Image x:Name="starSelectedOne" Source="star_selected.png"/>
	      </Grid>
	      <Grid >
	        <Image x:Name="starBlankTwo" Source="star_outline.png" />
	        <Image x:Name="starSelectedTwo" Source="star_selected.png"/>
	      </Grid>
	      <Grid >
	        <Image x:Name="starBlankThree" Source="star_outline.png" /> 
	        <Image x:Name="starSelectedThree" Source="star_selected.png"/>
	      </Grid>
	      <Grid >
	        <Image x:Name="starBlankFour" Source="star_outline.png" />
	        <Image x:Name="starSelectedFour" Source="star_selected.png"/>
	      </Grid>
	      <Grid >
	        <Image x:Name="starBlankFive" Source="star_outline.png" />
	        <Image x:Name="starSelectedFive" Source="star_selected.png"/>
	      </Grid>
	    </StackLayout>
	</ContentView.Content>
</ContentView>
